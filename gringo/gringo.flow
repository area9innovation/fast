import gringo/gringo_pretty;
import gringo/gringo_grammar;
import gringo/gringo_parse;

main() {
	grammar2 = gringoGrammar();

	grammar = 
		gseq([
			GBind("exp", 
				gchoices([
					gseq([
						GVar("exp", 1),
						GString("+"),
						GVar("exp", 2),
						GUnquote(GString("+")),
					]),
					gseq([
						GVar("exp", 3),
						GString("*"),
						GVar("exp", 4),
						GUnquote(GString("*")),
					]),
					GSeq(
						GRange(48, 57),
						GUnquote(GString("1")),
					)
				]),
			),
			GVar("exp", 0)
		]);

	env = GringoEnv(
		"1+3", makeTree(), makeTree(), 0, "", false,
	);

	genv = gringoParse(env, grammar);

	println("");

	if (genv.fail) {
		println("Parsing failed");
	}
	remaining = strRight(env.input, genv.i);
	if (remaining != "") {
		println("Did not parse: " + remaining);
	}
	println("Output: " + genv.output);
	quit(0);
}
