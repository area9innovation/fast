exp = exp "||" exp $"||"
	|> exp "&&" exp $"&&"
	|> exp "==" exp $"==" | exp "!=" exp $"!="
	|> exp ("<=" | "<" | ">=" | ">") exp
	|> exp ("+" exp $"+" | "-" exp $"-")*
	|> exp ("*" exp $"*" | "/" exp $"/" | "%" exp $"%")*

	|> exp "[" exp "]" $"index"
	|> exp "." exp $"dot"
	|> exp "?" exp ":" exp $"ifelse"

	|> (
		"if" exp exp "else" exp $"ifelse" 
		| "if" exp exp $"if"
		| "(" exp ")"
		| "-" exp $"negate"
		| $('0x30'-'0x39'+)
	);
exp
