import plow/bexp/bexp;
import plow/dexp/dexp;
import plow/types/type;

export {
	// Typed module
	BModule(
		flowpath : string, 
		fullpath : string,
		imports : [DImport], 
		forbids : [DForbid],
		exported : DExport, 
		structs : Tree<string, DStruct>,
		unions : Tree<string, DUnion>,
		natives : Tree<string, BNative>,
		globals : Tree<string, BGlobal>, 
		// The order of globals in the source code
		order : [string]
	);

	BGlobal(id : string, typars : [string], recursive : bool, value : BExp, pos : int);
	BNative(id : string, isIo : bool, type : BType, name : string, fallback : Maybe<BGlobal>, pos : int);
}
