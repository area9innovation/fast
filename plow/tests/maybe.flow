
Maybe<?> ::= None, Some<?>;

None();
Some(value : ?);

either(m : Maybe<?>, def : ?) -> ? {
	switch (m) {
		None(): {
			// m : None<?>
			def;
		}
		Some(v): {
			// m : Some<?>
			m.value;
		}
	}
}

maybeBind(m : Maybe<?>, f: (?)->Maybe<??>) -> Maybe<??> {
	switch (m : Maybe) {
		None(): None();
		Some(v): f(v);
	}
}
