import mini/mini_db;

export {
	pushMiniValue(db : MiniDb, val : MiniValue) -> MiniDb;
	popMiniValue(db : MiniDb) -> Pair<MiniValue, MiniDb>;
}

pushMiniValue(db : MiniDb, val : MiniValue) -> MiniDb {
	MiniDb(db with stack = Cons(val, db.stack));
}

popMiniValue(db : MiniDb) -> Pair<MiniValue, MiniDb> {
	switch (db.stack) {
		EmptyList(): Pair(MiniInt(-1), db)
		Cons(h, t): {
			Pair(h, MiniDb(db with stack = t))
		}
	}
}
