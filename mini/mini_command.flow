import mini/mini_ast;
import ds/set;

export {
	MiniCommand ::= MiniFilename, MiniDefine, MiniReprocess;

		// Read this file and run it
		MiniFilename(name : string);

		// Define this id to this expression
		MiniDefine(file : string, name : string, value : MiniExp);

		// Re-process these ids, since their dependents are changed
		MiniReprocess(ids : Set<string>);
}
