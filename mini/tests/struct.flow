native println : io (flow) -> void = Native.println;

Maybe ::= None, Some<?>;
	None();
	Some(value : ?);


either(m, alternative) {
	switch (m : Maybe) {
		None(): alternative;
		Some(v): v;
	}
}


main() {
	None();
	s = Some(1);
	println(s.value);
	println(either(s, 14));
	m : Maybe = s;
	println(switch (m) {
		None(): 0;
		Some(v): v;
	});
//	println(s ?? { 1; } : 0);

//	e = Some(s with value = 2);
	e;
}
