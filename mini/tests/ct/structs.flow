native println : io (flow) -> void = Native.println;

Maybe<?> ::= None, Some<?>;

None();
Some(value : ?);
either(m : Maybe<?>, def : ?) -> ? {
	switch (m) {
		None(): {
			// m : None<?>
			def;
		}
		Some(v): {
			// m : Some<?>
			v;
//			m.value;
		}
	}
}

main() {
	__compiletime({
		println(None()); // None
		println(Some(1));	// Some(1)
		println(Some(1).value);	// 1
		println(either(None(), 0));	// 0
		println(either(Some(1), 0)); // 1
		0;
	});
}
